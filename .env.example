# Telescan Environment Configuration
# Copy this file to .env and fill in your values

# Telegram Bot Token (required)
# Get it from @BotFather on Telegram
BOT_TOKEN=your_telegram_bot_token_here

# ClamAV Configuration (optional)
# ClamAV daemon host and port
CLAMD_HOST=localhost
CLAMD_PORT=3310

# VirusTotal API Key (optional)
# Get it from https://www.virustotal.com/
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Google Safe Browsing API Key (optional)
# Get it from https://console.cloud.google.com/
GOOGLE_SAFE_BROWSING_API_KEY=your_google_api_key_here

# Logging Level (optional)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
